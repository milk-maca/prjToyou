                                            <th>가입날짜</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    <c:forEach items="${userList}" var="list">
                                    
                                        <tr class="odd gradeX">
                                            <td>${list.userId }</td>
                                            <td>${list.userName}</td>
                                            <td><a href="${list.userChannelLink }" target="_blank">${list.userChannelName }</a></td>
                                            <td>${list.userEmail }</td>
                                            <td>${list.userIspayment }</td>
                                            <td>${list.userJoindate }</td>
                                        </tr>
                                    </c:forEach>
                                    </tbody>
                                </table>
                            </div>
                            
                        </div>
                    </div>
